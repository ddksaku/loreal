      <Query>
        <DataSourceName>LorealOptimise</DataSourceName>
        <CommandText>declare @t table (customerGroup nvarchar(255))

insert into @t values ('All groups')

Insert into @t
SELECT DISTINCT CustomerGroup_Name FROM dbo.VersionSnapshot WHERE SalesDrive_Name = @salesDrive

select customerGroup as CustomerGroup  from @t</CommandText>
