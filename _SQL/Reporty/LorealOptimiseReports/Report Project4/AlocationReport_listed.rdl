        </Field>
        <Field Name="AnimationID">
          <DataField>AnimationID</DataField>
          <rd:TypeName>System.Guid</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>LorealOptimise</DataSourceName>
        <CommandText>SELECT DISTINCT Animation_Name AS AnimationName, Animation_ID AS AnimationID  FROM dbo.VersionSnapshot
